pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'SonarQube') { 
          ./mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:9.3.0.51899:sonar
        }
      }
    }
  }
}
