pipeline {
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'SonarQube') { 
          sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:9.3.0.51899:sonar'
        }
      }
    }
  }
}